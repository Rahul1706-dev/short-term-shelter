   "scripts": {
      "dev": "nodemon src/index.ts",
      "start": "node dist/index.js",
      "lint": "eslint src/ --quiet --fix",
      "build": "tsc -p ./",
      "prebuild": "npm run clean && npm run lint",
      "copy-schema": "copyfiles --up 2 src/graphql/schema.graphql dist/graphql/",
      "clean": "rm -rf ./dist",
      "seed": "ts-node src/temp/seed.ts",
      "clear": "ts-node temp/clear.ts",
      "codegen": "nodemon --exec \"graphql-codegen --config codegen.yml\" --watch src/graphql/typedefs.ts",
      "codegen:watch": "nodemon --exec \"yarn codegen\" --watch src/graphql/typedefs.ts"
   }
